{% extends 'site/layout.html.twig' %}

{% block title %}<title>{{ settings.name }} | jaspevoyance.com</title>{% endblock %}

{% block app_cart %}{% endblock %}

{% block app_navbar_second %}
    {% if not is_mobile() %}
        {% if config.get.activeRencontre or config.get.activeVoyance %}
            <div id="carousel-example-1z" class="carousel slide" data-ride="carousel">

                <!--Indicators-->
                <ol class="carousel-indicators">
                    {% if config.get.activeRencontre %}
                        <li data-target="#carousel-example-1z" data-slide-to="0" class="active"></li>
                        <li data-target="#carousel-example-1z" data-slide-to="1"></li>

                        {% if config.get.activeVoyance %}
                            <li data-target="#carousel-example-1z" data-slide-to="2"></li>
                            <li data-target="#carousel-example-1z" data-slide-to="3"></li>
