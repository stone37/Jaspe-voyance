                    {% else %}
                        <li data-target="#carousel-example-1z" data-slide-to="0" class="active"></li>
                        <li data-target="#carousel-example-1z" data-slide-to="1"></li>
                    {% endif %}
                </ol>
                <!--/.Indicators-->

                <!--Slides-->
                <div class="carousel-inner" role="listbox">
                    {% if config.get.activeRencontre %}
                        <div class="carousel-item active">
                            <div class="view" style="background-image: url('https://mdbootstrap.com/img/Photos/Others/nature7.jpg'); background-repeat: no-repeat; background-size: cover;">
                                <div class="mask rgba-black-light d-flex justify-content-center align-items-center">
                                    <div class="text-center white-text mx-5 wow fadeIn">
                                        <h1 class="mt-4 mb-5 bg-primary px-4 py-2">
                                            <strong class="font-weight-600">Envie de vous faire des amis ?</strong>
                                        </h1>

                                        <a href="{{ path('app_event_index') }}" class="btn btn-primary">
                                            En savoir plus
                                            <i class="fas fa-angle-right ml-2"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="carousel-item">
                            <div class="view" style="background-image: url('https://mdbootstrap.com/img/Photos/Others/images/77.jpg'); background-repeat: no-repeat; background-size: cover;">
                                <div class="mask rgba-black-light d-flex justify-content-center align-items-center">
                                    <div class="text-center white-text mx-5 wow fadeIn">
                                        <h1 class="mt-4 mb-5 bg-primary px-4 py-2">
                                            <strong class="font-weight-600">Et plus ?</strong>
                                        </h1>

                                        <a href="{{ path('app_event_index') }}" class="btn btn-primary">
                                            En savoir plus
                                            <i class="fas fa-angle-right ml-2"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                    {% if config.get.activeVoyance %}
                        <div class="carousel-item {% if not config.get.activeRencontre %}active{% endif %}">
                            <div class="view" style="background-image: url('https://mdbootstrap.com/img/Photos/Others/images/47.jpg'); background-repeat: no-repeat; background-size: cover;">
                                <div class="mask rgba-black-light d-flex justify-content-center align-items-center">
                                    <div class="text-center white-text mx-5 wow fadeIn">
                                        <h1 class="mt-4 mb-5 bg-primary px-4 py-2">
                                            <strong class="font-weight-600">Vous etre face à une situation délicate ...</strong>
                                        </h1>

                                        <a href="{{ path('app_voyance_index') }}" class="btn btn-primary">
                                            En savoir plus
                                            <i class="fas fa-angle-right ml-2"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <div class="view" style="background-image: url('https://mdbootstrap.com/img/Photos/Others/images/47.jpg'); background-repeat: no-repeat; background-size: cover;">
                                <div class="mask rgba-black-light d-flex justify-content-center align-items-center">
                                    <div class="text-center white-text mx-5 wow fadeIn">
                                        <h1 class="mt-4 mb-5 bg-primary px-4 py-2">
                                            <strong class="font-weight-600">Vous ne saviez plus quoi faire ?</strong>
                                        </h1>

                                        <a href="{{ path('app_voyance_index') }}" class="btn btn-primary">
                                            En savoir plus
                                            <i class="fas fa-angle-right ml-2"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endif %}

                </div>
                <a class="carousel-control-prev" href="#carousel-example-1z" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carousel-example-1z" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        {% endif %}
    {% endif %}
{% endblock %}

